{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "LettuceEncrypt": "Debug",
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "Microsoft.AspNetCore.HttpLogging": "Information",
        "System": "Warning",
        "Thinktecture": "Debug",
        "Thinktecture.Relay.Server.Middleware": "Verbose"
      }
    },
    "Enrich": [
      "FromLogContext"
    ]
  },
  "LettuceEncrypt": {
    "AcceptTermsOfService": true
  },
  "RabbitMq": {
    "Uri": "amqp://relayserver:<Strong!Passw0rd>@localhost"
  },
  "Authentication": {
    "Authority": "http://localhost:5070"
  },
  "Kestrel": {
    "Limits": {
      "MaxRequestBodySize": 115343360
    }
  },
  "Maintenance": {
  },
  "Statistics": {
    "EnableConnectionCleanup": false
  },
  "RelayServer": {
  },
  "CertificateStore": {
    "Path": "./"
  },
  "Tenants": [
    {
      "id": "aaaaaaaa-aaaa-aaaa-aaaa-aaaaaaaaaaaa",
      "name": "TestTenant1",
      "clientSecret": "<Strong!Passw0rd>"
    },
    {
      "id": "bbbbbbbb-bbbb-bbbb-bbbb-bbbbbbbbbbbb",
      "name": "TestTenant2",
      "clientSecret": "<Strong!Passw0rd>"
    }
  ]
}
